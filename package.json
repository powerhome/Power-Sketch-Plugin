{
  "name": "Power",
  "version": "8.0.0",
  "description": "Power Home Remodeling data supplier plugin",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "phrg",
    "manifest": "src/manifest.json",
    "main": "phrg.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhome/sketch-data-plugin.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4"
  },
  "author": "Power Home Remodeling Group, LLC <christina.tai@powerhrg.com>"
}
